{
  "name": "pgdb-backup-cli",
  "version": "0.0.1",
  "description": "A CLI tool for backing up PostgreSQL Docker containers",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "test": "cross-env TS_NODE_PROJECT='./tsconfig.test.json' mocha",
    "ci": "pnpm run build && pnpm run test",
    "commit": "cz"
  },
  "keywords": [
    "cli",
    "backup-postgres",
    "backup-postgres-containers",
    "docker",
    "postgres",
    "backup"
  ],
  "repository": {
    "url": "https://github.com/shaiknoorullah/pgdb-backup-cli.git"
  },
  "author": "Shaik Noorullah, https://github.com/shaiknoorullah/, shaiknooru247@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^12.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/release-notes-generator": "^13.0.0",
    "@types/chai": "^4.3.16",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.12",
    "@types/sinon": "^17.0.3",
    "chai": "4.4.1",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "husky": "^9.0.11",
    "mocha": "^10.4.0",
    "semantic-release": "^23.1.1",
    "sinon": "^18.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
